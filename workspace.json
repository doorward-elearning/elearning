        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/edudoor-node-backend/.eslintrc",
            "tsConfig": ["apps/edudoor-node-backend/tsconfig.app.json", "apps/edudoor-node-backend/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "!apps/apps/edudoor-node-backend/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/edudoor-node-backend/jest.config.js",
            "tsConfig": "apps/edudoor-node-backend/tsconfig.spec.json"
          }
        }
      }
    },
