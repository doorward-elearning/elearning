              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "doorward-frontend:build",
            "port": 3000,
            "host": "0.0.0.0",
            "allowedHosts": "doorward.local"
          },
          "configurations": {
            "production": {
              "buildTarget": "doorward-frontend:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
