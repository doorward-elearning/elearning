          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/doorward-frontend",
            "index": "apps/doorward-frontend/src/index.html",
            "main": "apps/doorward-frontend/src/main.tsx",
            "polyfills": "apps/doorward-frontend/src/polyfills.ts",
