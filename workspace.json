{
  "version": 1,
  "projects": {
    "edudoor-frontend": {
      "root": "apps/edudoor-frontend",
      "sourceRoot": "apps/edudoor-frontend/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/edudoor-frontend",
            "index": "apps/edudoor-frontend/src/index.html",
            "main": "apps/edudoor-frontend/src/main.tsx",
            "polyfills": "apps/edudoor-frontend/src/polyfills.ts",
            "tsConfig": "apps/edudoor-frontend/tsconfig.app.json",
            "assets": ["apps/edudoor-frontend/src/favicon.ico", "apps/edudoor-frontend/src/assets"],
            "styles": ["apps/edudoor-frontend/src/styles.scss"],
            "scripts": [],
            "webpackConfig": "webpack.config.js"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/edudoor-frontend/src/environments/environment.ts",
                  "with": "apps/edudoor-frontend/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "edudoor-frontend:build",
            "port": 3000,
            "host": "0.0.0.0"
          },
          "configurations": {
            "production": {
              "buildTarget": "edudoor-frontend:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/edudoor-frontend/.eslintrc",
            "tsConfig": ["apps/edudoor-frontend/tsconfig.app.json", "apps/edudoor-frontend/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "!apps/edudoor-frontend/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/edudoor-frontend/jest.config.js",
            "tsConfig": "apps/edudoor-frontend/tsconfig.spec.json"
          }
        }
      }
    },
    "edudoor-frontend-e2e": {
      "root": "apps/edudoor-frontend-e2e",
      "sourceRoot": "apps/edudoor-frontend-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/edudoor-frontend-e2e/cypress.json",
            "tsConfig": "apps/edudoor-frontend-e2e/tsconfig.e2e.json",
            "devServerTarget": "edudoor-frontend:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "edudoor-frontend:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/edudoor-frontend-e2e/.eslintrc",
            "tsConfig": ["apps/edudoor-frontend-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/edudoor-frontend-e2e/**"]
          }
        }
      }
    },
    "ui": {
      "root": "libs/ui",
      "sourceRoot": "libs/ui/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
